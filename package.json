{
  "name": "keyway-api",
  "version": "0.1.0",
  "description": "GitHub-native secrets manager API",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "validate": "bash scripts/pre-push.sh",
    "deploy:check": "pnpm run type-check && pnpm run build"
  },
  "keywords": [
    "secrets",
    "github",
    "api",
    "encryption"
  ],
  "author": "Nicolas Ritouet",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "10.3.0",
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.7",
    "fastify": "^5.6.2",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.3",
    "posthog-node": "^3.5.0",
    "resend": "^6.5.2",
    "stripe": "^20.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.14",
    "drizzle-kit": "^0.31.7",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
